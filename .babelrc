{
  "plugins": [
    "./tooling/babel-plugin-inject-package-version",
    ["babel-plugin-transform-builtin-extend", {
        "globals": ["Error"]
    }],
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-export-extensions",
    "lodash",
    "transform-runtime"
  ],
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["safari >= 7", "ie >= 11", "> 1%"]
      }
    }]
  ],
  "sourceMaps": true
}
